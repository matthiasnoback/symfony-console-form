{
    "name": "matthiasnoback/symfony-console-form",
    "type": "library",
    "description": "Use Symfony forms for Console command input",
    "keywords": ["Symfony", "console", "form"],
    "homepage": "http://github.com/matthiasnoback/symfony-console-form",
    "license": "MIT",
    "authors": [
        {
            "name": "Matthias Noback",
            "email": "matthiasnoback@gmail.com",
            "homepage": "http://php-and-symfony.matthiasnoback.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "Matthias\\SymfonyConsoleForm\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Matthias\\SymfonyConsoleForm\\Tests\\": "test/"
        }
    },
    "require": {
        "php": ">=8.0",
        "symfony/form": "~5.0 || ~6.0",
        "symfony/console": "~5.0 || ~6.0",
        "symfony/translation": "~5.0 || ~6.0",
        "symfony/intl": "^6.1"
    },
    "require-dev": {
        "beberlei/assert": "~2.1",
        "behat/behat": "^3.6",
        "symfony/finder": "~5 || ~6.0.0",
        "symfony/framework-bundle": "~5.0 || ~6.0",
        "symfony/validator": "~5.0 || ~6.0",
        "symfony/yaml": "~5.0 || ~6.0",
        "symfony/security-csrf": "~5.0 || ~6.0",
        "phpunit/phpunit": "^9.5",
        "symplify/easy-coding-standard": "^9.4",
        "symplify/coding-standard": "^9.4",
        "symfony/intl": "^5"
    }
}
